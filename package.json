{
  "dependencies": {
    "electron-is-dev": "^1.2.0",
    "express": "^4.17.1",
    "lodash": "^4.17.19",
    "remote": "^0.2.6"
  },
  "main": "./electron.js",
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-proposal-optional-chaining": "^7.10.4",
    "@babel/preset-env": "^7.10.3",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "electron": "^9.0.3",
    "electron-builder": "^22.7.0",
    "electron-packager": "^15.0.0",
    "parcel-bundler": "^1.12.4",
    "wait-on": "^5.0.1"
  },
  "staticFiles": {
    "staticPath": "data",
    "watcherGlob": "**"
  },
  "engines": {
    "node": ">=12.x",
    "electron": ">=9.x"
  },
  "browserslist": [
    "last 1 Chrome versions"
  ],
  "build": {
    "appId": "rumpus.xyz.electron",
    "mac": {
      "category": "your.app.category.type"
    },
    "files": [
      "electron.js",
      "build/*",
      "build/**/*"
    ]
  },
  "scripts": {
    "serv": "node app.js",
    "dev": "parcel ./src/index.html -d build/",
    "start-dev": "ELECTRON_START_URL=http://localhost:1234 electron .",
    "start": "electron .",
    "build": "parcel build ./src/index.html -d build/ --public-url ./",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "name": "Rumpus!",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryanmcgary/wss.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ryanmcgary/wss/issues"
  },
  "homepage": "https://github.com/ryanmcgary/wss#readme",
  "description": ""
}
